# This file is being used as a test in its own and also as a helper for project_structure.aria
struct Foo {
    func prettyprint() {
        return "I'm a Foo";
    }
}

func main() {
    val l = [1,2, alloc(Foo), false, 3.14f];

    assert prettyprint(l) == "[1, 2, I'm a Foo, false, 3.14]";
}
