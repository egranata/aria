import Map from aria.structures.map;
import aria.structures.hash.list;

func main() {
    val m = Map.new();
    val l1 = [1,2,3];
    val l2 = [4,5,6];
    m[l1] = 1+2+3;
    m[l2] = 4+5+6;

    assert m[l1] == 6;
    assert m[l2] == 15;

    assert m.get([1,2,3])!! == 6;
    assert m.get([4,5,6])!! == 15;

    assert m.get([3,2,1]).is_None();
    assert m.get([]).is_None();

    assert m.len() == 2;
}
