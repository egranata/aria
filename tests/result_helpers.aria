import ok, err, Result from aria.core.result;

val r1 = ok(42);
val r2 = err("blah");

func main() {
    assert r1.apply(|x| => x + 1) == ok(43);
    assert r2.apply(|x| => x + 1) == err("blah");

    assert r1.unwrap_or(100) == 42;
    assert r2.unwrap_or(100) == 100;
}
