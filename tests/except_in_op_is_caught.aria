struct InvalidArgument {}

struct Divider {
    operator /(rhs) {
        if rhs == 0 {
            throw alloc(InvalidArgument);
        } else {
            return 1;
        }
    }
}

func main() {
    val caught = false;

    try {
        alloc(Divider) / 0;
    } catch e {
        caught = e isa InvalidArgument;
    }

    assert caught;
}
