struct StoreFive {
    operator <= (x) {
        if x isa Int {
            return 5 <= x;
        } else {
            throw alloc(Unimplemented);
        }
    }

    operator >= (x) {
        if x isa Int {
            return 5 >= x;
        } else {
            throw alloc(Unimplemented);
        }
    }
}

func main() {
    assert alloc(StoreFive) >= 3;
    assert !(alloc(StoreFive) >= 7);
    assert alloc(StoreFive) >= 5;

    assert 8 >= alloc(StoreFive);
    assert !(4 >= alloc(StoreFive));
    assert 5 >= alloc(StoreFive);
}
