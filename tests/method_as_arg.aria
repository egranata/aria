struct AddNumber {
    type func new(n) {
        return alloc(This) {
            .n = n,
        };
    }

    func add(x) {
        return this.n + x;
    }
}

func perform_op(f,n) {
    return f(n);
}

func main() {
    val add5 = AddNumber.new(5);
    assert perform_op(add5.add,4) == 9;
}
